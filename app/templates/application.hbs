<table class="table table-bordered">
  <thead>
    <tr>
      <th>AlbumId</th>
      <th>Title</th>
      <th>Image</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |item|}}
      <tr>
        <td scope="row">{{item.albumId}}</td>
        <td>{{item.title}}</td>
        <td>
          <img src={{item.thumbnailUrl}} height="50" width="50">
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
